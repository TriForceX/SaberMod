gfx/menus/menu_buttonback_new
{
	nopicmip
	nomipmaps
    {
        map gfx/menus/menu_buttonback_new
        blendFunc add
    }
    {
        map gfx/hud/static_menur
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll -1 0
    }
    {
        map gfx/hud/static_menur
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll 1 0
    }
    {
        map gfx/hud/static_menur
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll -1.3 0
    }
    {
        map gfx/hud/static_menur
        blendFunc GL_DST_COLOR GL_ONE
        tcMod scroll 1.3 0
    }
}

